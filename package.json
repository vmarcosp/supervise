{
  "name": "supervise-crawler",
  "version": "0.0.0",
  "description": "Supervise crawler",
  "esy": {
    "build": "refmterr dune build -p supervise-crawler",
    "buildDev": "pesy",
    "release": { "releasedBinaries": [ "App.exe" ] }
  },
  "buildDirs": {
    "src": {
      "require": [
        "lambdasoup", "cohttp", "cohttp-lwt", "cohttp-lwt-unix", "lwt",
        "lwt_ppx", "pastel.lib", "console.lib", "yojson"
      ],
      "includeSubdirs":"unqualified",
      "preprocess": [ "pps", "lwt_ppx" ],
      "main": "Index",
      "name": "App.exe"
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/base": "v0.13.0",
    "@opam/cohttp": "*",
    "@opam/cohttp-lwt-unix": "*",
    "@opam/dune": ">=1.6.0",
    "@opam/lambdasoup": "*",
    "@opam/lwt": "*",
    "@opam/lwt_ppx": "~1.2.4",
    "@opam/yojson": "1.7.0",
    "@reason-native/console": "^0.1.0",
    "@reason-native/pastel": "^0.2.3",
    "ocaml": "~4.8.1000"
  },
  "devDependencies": {
    "ocaml": "4.7.1004",
    "refmterr": "*",
    "pesy": "*",
    "@opam/merlin": "*"
  }
}
