    "@opam/dune": ">=1.6.0",
    "@opam/lambdasoup": "*",
    "@opam/lwt": "*",
    "@opam/lwt_ppx": "~1.2.4",
    "@opam/yojson": "1.7.0",
    "@reason-native/console": "^0.1.0",
    "@reason-native/pastel": "^0.2.3",
    "@reason-native/rely": "^3.1.0",
    "ocaml": "~4.8.1000"
  },
  "devDependencies": {
    "ocaml": "4.7.1004",
